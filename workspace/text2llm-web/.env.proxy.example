# Proxy runtime
TEXT2LLM_WEB_PROXY_PORT=8790
PROXY_ENABLED=true
PROXY_ALLOWED_ORIGIN=*
PROXY_BODY_LIMIT=128kb
PROXY_UPSTREAM_TIMEOUT_MS=120000

# Cost controls
PROXY_RATE_LIMIT_RPM=20
PROXY_DAILY_TOKEN_CAP=100000

# Dev convenience
PROXY_SKIP_AUTH=true

# Supabase auth + telemetry (required when PROXY_SKIP_AUTH=false)
SUPABASE_URL=https://YOUR_PROJECT_ID.supabase.co
SUPABASE_ANON_KEY=YOUR_SUPABASE_ANON_KEY
SUPABASE_SERVICE_ROLE_KEY=YOUR_SUPABASE_SERVICE_ROLE_KEY

# Provider endpoints
PROXY_PROVIDER_OPENAI_CHAT_URL=https://api.openai.com/v1/chat/completions
PROXY_PROVIDER_OPENAI_EMBEDDINGS_URL=https://api.openai.com/v1/embeddings
PROXY_PROVIDER_OPENROUTER_CHAT_URL=https://openrouter.ai/api/v1/chat/completions

# Optional providers (set URLs to enable)
PROXY_PROVIDER_ANTHROPIC_CHAT_URL=
PROXY_PROVIDER_GOOGLE_CHAT_URL=
PROXY_PROVIDER_GOOGLE_EMBEDDINGS_URL=
PROXY_ANTHROPIC_VERSION=2023-06-01

# Kill switches
PROXY_DISABLE_OPENAI=false
PROXY_DISABLE_OPENROUTER=false
PROXY_DISABLE_ANTHROPIC=false
PROXY_DISABLE_GOOGLE=false
